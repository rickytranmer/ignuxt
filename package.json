{
  "name": "ignuxt-template",
  "version": "1.0.0",
  "description": "ignite nuxt template",
  "repository": {
    "type": "git",
    "url": "git@github.com:rickytranmer/ignuxt.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "open http://localhost:3000/ && npm run nuxt",
    "nuxt": "nuxt",
    "cross-env": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt",
    "build": "nuxt generate",
    "build:all": "npm run build:server && npm run build:api && npm run build:nuxt",
    "build:api": "tsc -p tsconfig.json && jsmin -o api/index.min.js api/index.js",
    "build:nuxt": "nuxt build",
    "build:server": "tsc server.ts && jsmin -o server.min.js server.js",
    "build:test": "npm run build:api && npm test",
    "serve": "nuxt start",
    "server": "NODE_ENV=production node server.min.js",
    "test": "date && mocha api/index.test.js --timeout 10000",
    "postinstall": "touch ./.env"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/gtm": "2.2.3",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "@nuxtjs/style-resources": "1.0.0",
    "@nuxtjs/vendor": "^1.1.7",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "minimist": ">=0.2.1 <1.0.0 || >=1.2.3",
    "nuxt-ssr-cache": "^1.5.1",
    "nuxt": "^2.12.0"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^3.0.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "jsmin": "^1.0.1",
    "mocha": "^7.0.1",
    "node-sass": "^4.12.0",
    "node-zopfli-es": "^1.0.4",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "shrink-ray-current": "^4.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "zlib": "^1.0.5"
  },
  "author": "ricky.tranmer",
  "license": "ISC"
}
